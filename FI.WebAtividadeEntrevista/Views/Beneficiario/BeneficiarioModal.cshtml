<div class="modal" id="modalBeneficiarios" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">
                    Beneficiários
                    <button type="button" class="close" data-dismiss="modal" aria-label="Fechar">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </h4>
            </div>

            <div class="modal-body">
                <div class="beneficiario-modal-container form-group">
                    <div class="row">
                        <div class="col-md-5 beneficiario-input-group">
                            <label for="cpf">CPF:</label>
                            <input type="text" id="CPFBeneficiario" class="form-control" placeholder="Ex.: 010.011.111-00" maxlength="15">
                        </div>

                        <div class="col-md-5 beneficiario-input-group">
                            <label for="nome">Nome:</label>
                            <input type="text" id="NomeBeneficiario" class="form-control" placeholder="Ex.: Maria" maxlength="50">
                        </div>

                        <div class="col-md-2">
                            <div class="form-group">
                                <label class="small">&nbsp;</label>
                                <button id="incluir-button" type="submit" class="btn btn-success form-control">
                                    Incluir
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="table-container">
                    <table class="table hidden">
                        <thead>
                            <tr>
                                <th scope="col" width="35%">CPF</th>
                                <th scope="col" width="35%">Nome</th>
                                <th scope="col">Ações</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>